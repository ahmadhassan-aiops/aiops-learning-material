def string_copies(string, n):
    if n < 0:
        return "n must be a non-negative integer."
    return string * n

string_copies('This is my sample string ', -3)

string_copies('This is my sample string ', 3)

"""### Q2"""

def check_even_odd():
    number = eval(input("Enter a number: "))
    if number % 2 == 0:
        print(f"{number} is even.")
    else:
        print(f"{number} is odd.")

check_even_odd()

"""### Q3"""

def is_within_100(n):
    return abs(1000 - n) <= 100 or abs(2000 - n) <= 100

print(is_within_100(950))

