# 2. Test whether none of the elements of a given array is zero
print("2. Test whether none of the elements of a given array is zero:")
array = np.array([1, 2, 3, 4, 5])
result = np.all(array != 0)
print(result)
print()

# 3. Test whether any of the elements of a given array is non-zero
print("3. Test whether any of the elements of a given array is non-zero:")
array = np.array([0, 0, 0, 1])
result = np.any(array != 0)
print(result)
print()

# 4. Compute the multiplication of two given matrices
print("4. Compute the multiplication of two given matrices:")
matrix1 = np.array([[1, 2], [3, 4]])
matrix2 = np.array([[5, 6], [7, 8]])
result = np.dot(matrix1, matrix2)
print(result)
print()

# 5. Compute the inner product of vectors for 1-D arrays (without complex conjugation) and in higher dimensions
print("5. Compute the inner product of vectors for 1-D arrays (without complex conjugation) and in higher dimensions:")
vec1 = np.array([1, 2, 3])
vec2 = np.array([4, 5, 6])
inner_product_1d = np.inner(vec1, vec2)
print("Inner product of 1-D arrays:", inner_product_1d)

mat1 = np.array([[1, 2], [3, 4]])
mat2 = np.array([[5, 6], [7, 8]])
inner_product_2d = np.inner(mat1, mat2)
print("Inner product of higher dimensions:", inner_product_2d)
print()

# 6. Compute the inverse of a given matrix
print("6. Compute the inverse of a given matrix:")
matrix = np.array([[1, 2], [3, 4]])
inverse_matrix = np.linalg.inv(matrix)
print(inverse_matrix)
print()

# 7. Generate five random numbers from the normal distribution
print("7. Generate five random numbers from the normal distribution:")
random_numbers = np.random.normal(size=5)
print(random_numbers)
print()

# 8. Generate six random integers between 10 and 30
print("8. Generate six random integers between 10 and 30:")
