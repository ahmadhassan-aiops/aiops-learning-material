print(random_integers)
print()

# 9. Create a 3x3x3 array with random values
print("9. Create a 3x3x3 array with random values:")
random_array = np.random.random((3, 3, 3))
print(random_array)
print()

# 10. Sort a given array of shape (2, 2) along the first axis, last axis, and on the flattened array
print("10. Sort a given array of shape (2, 2) along the first axis, last axis, and on the flattened array:")
array = np.array([[10, 40], [30, 20]])
print("Original array:")
print(array)

# Sort along the first axis
sorted_first_axis = np.sort(array, axis=0)
print("Sort the array along the first axis:")
print(sorted_first_axis)

# Sort along the last axis
sorted_last_axis = np.sort(array, axis=1)
print("Sort the array along the last axis:")
print(sorted_last_axis)

# Sort the flattened array
sorted_flattened = np.sort(array, axis=None)
print("Sort the flattened array:")
print(sorted_flattened)
print()

# 11. Create a structured array from given student names, heights, classes, and their data types, and sort the array by height
print("11. Create a structured array from given student names, heights, classes, and their data types, and sort the array by height:")
dtype = [('name', 'S10'), ('class', 'i4'), ('height', 'f4')]
values = [(b'James', 5, 48.5), (b'Nail', 6, 52.5), (b'Paul', 5, 42.1), (b'Pit', 5, 40.11)]
structured_array = np.array(values, dtype=dtype)

print("Original array:")
print(structured_array)

# Sort by height
sorted_array = np.sort(structured_array, order='height')
print("Sort by height:")
print(sorted_array)