"""## Q6"""

def sort_by_last_element(lst):
    return sorted(lst, key=lambda x: x[-1])

print(sort_by_last_element([(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]))

"""## Q7"""

def extract_chars(s):
    if len(s) < 2:
        return ""
    return s[:2] + s[-2:]

print(extract_chars('w3resource'))
print(extract_chars('w3'))
print(extract_chars(' w'))

"""## Q8"""

def length_of_longest_word(words):
    return len(max(words, key=len))

print(length_of_longest_word(['apple', 'banana', 'cherry']))
print(length_of_longest_word(['cat', 'dog', 'elephant']))

"""##Q9"""

def sort_dict_by_value(d, reverse=False):
    return dict(sorted(d.items(), key=lambda item: item[1], reverse=reverse))

print(sort_dict_by_value({1: 3, 2: 1, 3: 2}))
print(sort_dict_by_value({1: 3, 2: 1, 3: 2}, True))

"""## Q10"""

def sort_dict_by_key(d, reverse=False):
    return dict(sorted(d.items(), key=lambda item: item[0], reverse=reverse))

print(sort_dict_by_key({3: 'c', 1: 'a', 2: 'b'}))
print(sort_dict_by_key({3: 'c', 1: 'a', 2: 'b'}, True))

"""## Q11"""

def add_key_to_dict(d, key, value):
    d[key] = value
    return d

print(add_key_to_dict({0: 10, 1: 20}, 2, 30))

"""# Q12"""

def concatenate_dicts(*dicts):
    result = {}
    for d in dicts:
        result.update(d)
    return result

dic1 = {1: 10, 2: 20}
dic2 = {3: 30, 4: 40}
dic3 = {5: 50, 6: 60}
print(concatenate_dicts(dic1, dic2, dic3))

